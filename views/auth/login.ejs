<h1>Login Page</h1>


<% if(typeof errorMsg!== 'undefined'){ %>

 <div class="bg-danger">
    <%-errorMsg%>
 </div>
<% } %>
<form action="/login" method="post">
   
    <input type="text" name="username" >
    <input type="password" name="password" >
    <button type="submit" class="btn btn-primary">Login</button>
</form>